{
  "name": "stucco-js",
  "version": "0.10.4",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "type": "module",
  "bin": {
    "stucco-js": "lib/cli/cli.js",
    "stucco-js.cmd": "lib/cli/cli.cmd",
    "stucco": "lib/stucco/cmd.js",
    "stucco.cmd": "lib/stucco/cmd.cmd"
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -c jest.config.cjs",
    "test:e2e": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -c jest.e2e.config.cjs",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" \"e2e/**/*.ts\" \"*.js\"",
    "bump-stucco": "node scripts/bump_stucco.js",
    "clean": "rimraf lib/",
    "copyfiles": "copyfiles -u 1 src/cli/cli.cmd src/stucco/cmd.cmd  lib/",
    "build": "npm run clean && tsc && npm run copyfiles"
  },
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "bin-version-check": "^5.0.0",
    "google-protobuf": "^3.18.0",
    "grpc-health-check-ts": "^1.0.2",
    "lru-cache": "^6.0.0",
    "node-forge": "^1.3.1",
    "retry": "^0.13.1",
    "stucco-ts-proto-gen": "^0.7.21",
    "uuid": "^8.3.2",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.5",
    "@types/jest": "^27.0.2",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^16.10.2",
    "@types/node-forge": "^1.0.4",
    "@types/retry": "^0.12.2",
    "@types/uuid": "^8.3.1",
    "@types/yargs": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.11.2",
    "jest": "^27.3.1",
    "node-abort-controller": "^3.0.0",
    "node-fetch": "^3.2.10",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
